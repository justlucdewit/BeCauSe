import "std/io"

// Print the argument count
"Arguments:\n" IO_WRITE

// Loop over all arguments
0 while dup argc < do
    // Write list line
    " - " IO_WRITE

    // Print n'th terminal argument
    dup 8 * argv + @64 IO_WRITE_CSTR

    // Increment
    1 +
    
    // Print new line
    "\n" IO_WRITE
end